abstract class Vehicle {
    String brand;
    int year;
    public Vehicle(String brand, int year) {
        this.brand = brand;
        this.year = year;
    }
    abstract void startEngine();
    public void displayInfo() {
        System.out.println("Brand: " + brand + " | Year: " + year);
    }
}

interface Features {
    void gps();
    void parkAssist();
}

class Car extends Vehicle implements Features {
    public Car(String brand, int year) {
        super(brand, year);
    }
    @Override
    void startEngine() {
        System.out.println("Car engine started.");
    }
    @Override
    public void gps() {
        System.out.println("GPS is enabled.");
    }
    @Override
    public void parkAssist() {
        System.out.println("Parking assist active.");
    }
}

class Bike extends Vehicle {
    public Bike(String brand, int year) {
        super(brand, year);
    }
    @Override
    void startEngine() {
        System.out.println("Bike engine started.");
    }
}

public class AbstractionDemo {
    public static void main(String[] args) {
        Vehicle myCar = new Car("Toyota", 2024);
        myCar.displayInfo();
        myCar.startEngine();

        if (myCar instanceof Car) {
            Features carFeatures = (Car) myCar;
            carFeatures.gps();
            carFeatures.parkAssist();
        }

        Vehicle myBike = new Bike("Honda", 2023);
        myBike.displayInfo();
        myBike.startEngine();
    }
}
